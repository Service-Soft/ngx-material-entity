{
    "name": "ngx-material-entity",
    "version": "0.0.0",
    "scripts": {
        "stack": "run-s clear:dist stack:only",
        "showcase": "ng serve ngx-material-entity-showcase",
        "build:watch": "ng build ngx-material-entity --watch",
        "api": "nodemon --watch api-data.ts --watch api.ts api.ts --watch dist/ngx-material-entity/public-api.d.ts",
        "test": "jest --config projects/ngx-material-entity/jest.config.json",
        "e2e": "concurrently --kill-others 'npm run stack' 'run-s waitOn:server cy:open'",
        "e2e:run": "concurrently --success last --kill-others 'npm run build:watch' 'run-s waitOn:build api' 'run-s waitOn:build showcase' 'run-s waitOn:server cy:run'",
        "cy:run": "cypress run --browser chrome",
        "cy:open": "cypress open",
        "lint": "eslint projects --max-warnings 0",
        "lint:fix": "eslint projects --fix",
        "stack:only": "concurrently --kill-others 'npm run build:watch' 'run-s waitOn:build api' 'run-s waitOn:build showcase'",
        "clear:dist": "rimraf dist/ngx-material-entity",
        "waitOn:build": "wait-on dist/ngx-material-entity",
        "waitOn:server": "wait-on http://localhost:4200",
        "build": "ng build ngx-material-entity",
        "build:prod": "run-s build copyToDist",
        "copyToDist": "run-s copyToDist:readme copyToDist:contributing copyToDist:license",
        "copyToDist:readme": "ncp ./README.md ./dist/ngx-material-entity/README.md",
        "copyToDist:contributing": "ncp ./CONTRIBUTING.md ./dist/ngx-material-entity/CONTRIBUTING.md",
        "copyToDist:license": "ncp ./LICENSE.md ./dist/ngx-material-entity/LICENSE.md"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/cdk": "^13.3.9",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/material": "^13.3.9",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "bootstrap": "^5.1.3",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.7",
        "@angular-eslint/builder": "13.5.0",
        "@angular-eslint/eslint-plugin": "13.5.0",
        "@angular-eslint/eslint-plugin-template": "13.5.0",
        "@angular-eslint/schematics": "13.5.0",
        "@angular-eslint/template-parser": "13.5.0",
        "@angular/cli": "~13.3.5",
        "@angular/compiler-cli": "~13.3.0",
        "@types/jest": "^28.1.4",
        "@types/json-server": "^0.14.4",
        "@types/lodash": "^4.14.182",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "@typescript-eslint/parser": "5.27.1",
        "concurrently": "^7.2.2",
        "cypress": "latest",
        "eslint": "^8.17.0",
        "eslint-plugin-jsdoc": "^39.3.3",
        "jasmine-core": "~4.0.0",
        "json-server": "^0.17.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ncp": "^2.0.0",
        "ng-packagr": "^13.0.0",
        "nodemon": "^2.0.18",
        "npm-run-all": "^4.1.5",
        "ts-jest": "^28.0.4",
        "ts-node": "^10.8.2",
        "typescript": "~4.6.2",
        "wait-on": "^6.0.1"
    }
}