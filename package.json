{
    "name": "ngx-material-entity",
    "version": "0.0.0",
    "scripts": {
        "start": "npm run stack",
        "stack": "run-s clear:dist stack:only",
        "showcase": "ng serve ngx-material-entity-showcase",
        "build:watch": "ng build ngx-material-entity --watch",
        "api": "nodemon --watch api-data.ts --watch api.ts --watch dist/ngx-material-entity/public-api.d.ts api.ts",
        "test": "jest --config projects/ngx-material-entity/jest.config.ts",
        "e2e": "concurrently --kill-others 'npm run stack' 'run-s waitOn:server cy:open'",
        "e2e:run": "ts-node ./e2e-stack.ts",
        "cy:run": "cypress run --browser chrome",
        "cy:open": "cypress open",
        "lint": "eslint projects --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint projects --fix",
        "stack:only": "concurrently --kill-others 'npm run build:watch' 'run-s waitOn:build api' 'run-s waitOn:build showcase'",
        "clear:dist": "rimraf dist/ngx-material-entity",
        "waitOn:build": "wait-on dist/ngx-material-entity",
        "waitOn:server": "wait-on http://localhost:4200",
        "build": "ng build ngx-material-entity",
        "build:prod": "run-s build copyToDist",
        "copyToDist": "run-s copyToDist:readme copyToDist:contributing copyToDist:license",
        "copyToDist:readme": "ncp ./README.md ./dist/ngx-material-entity/README.md",
        "copyToDist:contributing": "ncp ./CONTRIBUTING.md ./dist/ngx-material-entity/CONTRIBUTING.md",
        "copyToDist:license": "ncp ./LICENSE.md ./dist/ngx-material-entity/LICENSE.md"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~14.2.1",
        "@angular/cdk": "^14.2.1",
        "@angular/common": "~14.2.1",
        "@angular/compiler": "~14.2.1",
        "@angular/core": "~14.2.1",
        "@angular/forms": "~14.2.1",
        "@angular/material": "^14.2.1",
        "@angular/platform-browser": "~14.2.1",
        "@angular/platform-browser-dynamic": "~14.2.1",
        "@angular/router": "~14.2.1",
        "bootstrap": "^5.2.1",
        "jszip": "^3.10.1",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.5.6",
        "tslib": "^2.4.0",
        "zone.js": "~0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~14.2.2",
        "@angular/cli": "~14.2.2",
        "@angular/compiler-cli": "~14.2.1",
        "@types/cross-spawn": "^6.0.2",
        "@types/jest": "^29.0.1",
        "@types/json-server": "^0.14.4",
        "@types/lodash": "^4.14.185",
        "@types/node": "^18.7.17",
        "concurrently": "^7.4.0",
        "cypress": "latest",
        "eslint-config-service-soft": "^1.0.8",
        "jest": "^29.0.3",
        "json-server": "^0.17.0",
        "ncp": "^2.0.0",
        "ng-packagr": "^14.2.1",
        "ngx-material-navigation": "^1.0.4",
        "nodemon": "^2.0.19",
        "npm-run-all": "^4.1.5",
        "ts-jest": "^29.0.0",
        "ts-node": "^10.9.1",
        "typescript": "~4.8.3",
        "wait-on": "^6.0.1"
    }
}
